{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "blank-full-width",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank Full Width"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
    "settings": {
        "appearanceTools": true,
		"border": {
			"radius": true,
			"color": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"button": true,
			"caption": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#000000", "#ffffff"],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": ["#350CF5", "#ffffff"],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": ["#E60067", "#ffffff"],
					"name": "Accent and White",
					"slug": "accent-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--base) 100%)",
					"name": "Base to Primary",
					"slug": "base-primary"
				},
				{
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--base) 100%)",
					"name": "Base to Accent",
					"slug": "base-accent"
				},
				{
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--primary) 50%, var(--wp--preset--color--base) 100%)",
					"name": "Base to Primary to Accent",
					"slug": "base-primary-accent"
				},
				{
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--accent) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Primary to Accent",
					"slug": "primary-accent"
				}
			],
			"heading": true,
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#ffffff"
				},
                {
                    "name": "Base",
                    "slug": "base",
                    "color": "#0A0230"
                },
                {
                    "name": "Base Light",
                    "slug": "base-light",
                    "color": "#F0EBF7"
                },
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#350CF5"
                },
                {
                    "name": "Primary Light",
                    "slug": "primary-light",
                    "color": "#00BAE6"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#F5D805"
                },
                {
                    "name": "Tertiary",
                    "slug": "tertiary",
                    "color": "#F57B0C"
                },
                {
                    "name": "Accent",
                    "slug": "accent",
                    "color": "#E60067"
                }
            ],
			"text": true
		},
		"custom": {
			"infinitum": {
				"contentWidth": 1200,
				"contentWidthMax": "62.5vw",
				"contentWidthPx": "calc(var(--wp--custom--infinitum--content-width) * 1px)",
				"contentWidthResponsiveRatio": 0.8,
				"contentWidthWideRatio": 1.2,
				"contentSize": "max(var(--wp--custom--infinitum--content-width-px), var(--wp--custom--infinitum--content-width-max))",
				"wideSize": "max(calc(var(--wp--custom--infinitum--content-width-px) * var(--wp--custom--infinitum--content-width-wide-ratio)), calc(var(--wp--custom--infinitum--content-width-max) * var(--wp--custom--infinitum--content-width-wide-ratio)))",
				"featuredImageDisplayRatio": 2,
				"featuredImageDisplayFit": "cover",
				"featuredImageSizeRatio": 0.666666,
				"featuredImageThumbnailDisplayRatio": 1.5,
				"featuredImageThumbnailDisplayFit": "cover",
				"typography": {
					"typeRatio": "calc(1 + (1 / 4))",
					"fontSizeRoot": 16,
					"fontSizeRootPx": "calc(var(--wp--custom--infinitum--typography--font-size-root) * 1px)",
					"fontSizeRootRem": "1rem",
					"fontSize100": "calc(var(--wp--custom--infinitum--typography--font-size-root-px) + 0.3vw)",
					"fontSize200": "calc(var(--wp--custom--infinitum--typography--font-size-100) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize300": "calc(var(--wp--custom--infinitum--typography--font-size-200) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize400": "calc(var(--wp--custom--infinitum--typography--font-size-300) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize500": "calc(var(--wp--custom--infinitum--typography--font-size-400) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize600": "calc(var(--wp--custom--infinitum--typography--font-size-500) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize700": "calc(var(--wp--custom--infinitum--typography--font-size-600) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize800": "calc(var(--wp--custom--infinitum--typography--font-size-700) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize900": "calc(var(--wp--custom--infinitum--typography--font-size-800) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize1000": "calc(var(--wp--custom--infinitum--typography--font-size-900) * var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize90": "calc(var(--wp--custom--infinitum--typography--font-size-100) / var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSize80": "calc(var(--wp--custom--infinitum--typography--font-size-90) / var(--wp--custom--infinitum--typography--type-ratio))",
					"fontSizeSubHeading": "var(--wp--custom--infinitum--typography--font-size-200)",
					"headingLineHeight": 1.1,
					"lineHeightRootRatio": 1.25,
					"lineHeight": "calc((100% * var(--wp--custom--infinitum--typography--line-height-root-ratio)) + 0.3vw)",
					"lineHeightPx": "calc(var(--wp--custom--infinitum--typography--font-size-root-px) * var(--wp--custom--infinitum--typography--line-height-root-ratio) + 0.3vw)"
				}
			}
		},
		"dimensions": {
			"aspectRatio": true,
			"minHeight": true
		},
        "layout": {
            "contentSize": "var(--wp--custom--infinitum--content-size)",
			"wideSize": "var(--wp--custom--infinitum--wide-size)"
        },
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Small Soft",
					"shadow": "0 0 var(--wp--preset--spacing--1-4) 0 rgba(0, 0, 0, 0.25), 0 0 calc(var(--wp--preset--spacing--1-4) / 2) 0 rgba(0, 0, 0, 0.1)",
					"slug": "small-soft"
				},
				{
					"name": "Small Glass",
					"shadow": "var(--wp--preset--spacing--1-4) var(--wp--preset--spacing--1-4) 0 0 rgba(0, 0, 0, 0.15)",
					"slug": "small-glass"
				},
				{
					"name": "Small Solid",
					"shadow": "var(--wp--preset--spacing--1-4) var(--wp--preset--spacing--1-4) 0 0 rgba(0, 0, 0, 1)",
					"slug": "small-solid"
				},
				{
					"name": "Medium Soft",
					"shadow": "0 0 var(--wp--preset--spacing--1-2) 0 rgba(0, 0, 0, 0.25), 0 0 calc(var(--wp--preset--spacing--1-2) / 2) 0 rgba(0, 0, 0, 0.1)",
					"slug": "medium-soft"
				},
				{
					"name": "Medium Glass",
					"shadow": "var(--wp--preset--spacing--1-2) var(--wp--preset--spacing--1-2) 0 0 rgba(0, 0, 0, 0.15)",
					"slug": "medium-glass"
				},
				{
					"name": "Medium Solid",
					"shadow": "var(--wp--preset--spacing--1-2) var(--wp--preset--spacing--1-2) 0 0 rgba(0, 0, 0, 1)",
					"slug": "medium-solid"
				},
				{
					"name": "Large Soft",
					"shadow": "0 0 var(--wp--preset--spacing--3-4) 0 rgba(0, 0, 0, 0.25), 0 0 calc(var(--wp--preset--spacing--3-4) / 2) 0 rgba(0, 0, 0, 0.1)",
					"slug": "large-soft"
				},
				{
					"name": "Large Glass",
					"shadow": "var(--wp--preset--spacing--3-4) var(--wp--preset--spacing--3-4) 0 0 rgba(0, 0, 0, 0.15)",
					"slug": "large-glass"
				},
				{
					"name": "Large Solid",
					"shadow": "var(--wp--preset--spacing--3-4) var(--wp--preset--spacing--3-4) 0 0 rgba(0, 0, 0, 1)",
					"slug": "large-solid"
				}
			]
		},
        "spacing": {
            "margin": true,
            "padding": true,
			"spacingScale": {
                "steps": 0
            },
			"spacingSizes": [
				{
					"name": "1 / 8",
					"size": "calc(var(--wp--style--block-gap) * 0.125)",
					"slug": "1-8"
				},
				{
					"name": "1 / 4",
					"size": "calc(var(--wp--style--block-gap) * 0.25)",
					"slug": "1-4"
				},
				{
					"name": "1 / 3",
					"size": "calc(var(--wp--style--block-gap) * 0.333333)",
					"slug": "1-3"
				},
				{
					"name": "1 / 2",
					"size": "calc(var(--wp--style--block-gap) * 0.5)",
					"slug": "1-2"
				},
				{
					"name": "2 / 3",
					"size": "calc(var(--wp--style--block-gap) * 0.666666)",
					"slug": "2-3"
				},
				{
					"name": "3 / 4",
					"size": "calc(var(--wp--style--block-gap) * 0.75)",
					"slug": "3-4"
				},
				{
					"name": "1",
					"size": "var(--wp--style--block-gap)",
					"slug": "1"
				},
				{
					"name": "2",
					"size": "calc(var(--wp--style--block-gap) * 2)",
					"slug": "2"
				},
				{
					"name": "3",
					"size": "calc(var(--wp--style--block-gap) * 3)",
					"slug": "3"
				},
				{
					"name": "4",
					"size": "calc(var(--wp--style--block-gap) * 4)",
					"slug": "4"
				},
				{
					"name": "5",
					"size": "calc(var(--wp--style--block-gap) * 5)",
					"slug": "5"
				},
				{
					"name": "6",
					"size": "calc(var(--wp--style--block-gap) * 6)",
					"slug": "6"
				},
				{
					"name": "7",
					"size": "calc(var(--wp--style--block-gap) * 7)",
					"slug": "7"
				},
				{
					"name": "8",
					"size": "calc(var(--wp--style--block-gap) * 8)",
					"slug": "8"
				},
				{
					"name": "9",
					"size": "calc(var(--wp--style--block-gap) * 9)",
					"slug": "9"
				},
				{
					"name": "10",
					"size": "calc(var(--wp--style--block-gap) * 10)",
					"slug": "10"
				}
			],
            "units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
        },
        "typography": {
			"defaultFontSizes": false,
			"dropCap": false,
            "fontFamilies": [
				{
                    "fontFace": [
                        {
                            "fontFamily": "Raleway",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "100 900",
                            "src": [
                                "file:./assets/fonts/raleway/Raleway-VariableFont_wght.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Raleway",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "100 900",
                            "src": [
                                "file:./assets/fonts/raleway/Raleway-Italic-VariableFont_wdth.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Raleway\", sans-serif",
                    "name": "Raleway",
                    "slug": "sans-serif-1"
                }
            ],
			"fontSizes": [
				{
					"name": "1000",
                    "size": "var(--wp--custom--infinitum--typography--font-size-1000)",
                    "slug": "1000"
                },
				{
					"name": "900",
                    "size": "var(--wp--custom--infinitum--typography--font-size-900)",
                    "slug": "900"
                },
				{
					"name": "800",
                    "size": "var(--wp--custom--infinitum--typography--font-size-800)",
                    "slug": "800"
                },
				{
					"name": "700",
                    "size": "var(--wp--custom--infinitum--typography--font-size-700)",
                    "slug": "700"
                },
				{
					"name": "600",
                    "size": "var(--wp--custom--infinitum--typography--font-size-600)",
                    "slug": "600"
                },
                {
					"name": "500 (H1)",
                    "size": "var(--wp--custom--infinitum--typography--font-size-500)",
                    "slug": "500"
                },
                {
					"name": "400 (H2)",
                    "size": "var(--wp--custom--infinitum--typography--font-size-400)",
                    "slug": "400"
                },
                {
					"name": "300 (H3)",
                    "size": "var(--wp--custom--infinitum--typography--font-size-300)",
                    "slug": "300"
                },
                {
					"name": "200 (H4)",
                    "size": "var(--wp--custom--infinitum--typography--font-size-200)",
                    "slug": "200"
                },
				{
					"name": "100 (Body)",
					"size": "var(--wp--custom--infinitum--typography--font-size-100)",
					"slug": "100"
				},
                {
                    "size": "var(--wp--custom--infinitum--typography--font-size-90)",
                    "slug": "90"
                },
                {
                    "size": "var(--wp--custom--infinitum--typography--font-size-80)",
                    "slug": "80"
                }
            ]
        },
		"useRootPaddingAwareAlignments": true
    },
    "styles": {
		"blocks":{
			"core/button": {
				"border": {
					"radius": {
						"topLeft": "0",
						"topRight": "0",
						"bottomRight": "0",
						"bottomLeft": "0"
					}
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--style--block-gap)",
						"right": "var(--wp--style--block-gap)",
						"bottom": "var(--wp--style--block-gap)",
						"left": "var(--wp--style--block-gap)"
					}
				},
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--style--block-gap)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--1-2)"
					}
				}
			},
			"core/list": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--style--block-gap)"
					}
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--style--block-gap)"
					}
				}
			},
			"core/separator": {
				"spacing": {
					"margin": {
						"top": "var(--wp--style--block-gap)",
						"bottom": "var(--wp--style--block-gap)"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0",
						"left": "0"
					}
				}
			},
			"core/navigation-link": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--1-4)",
						"right": "var(--wp--preset--spacing--1-4)",
						"bottom": "var(--wp--preset--spacing--1-4)",
						"left": "var(--wp--preset--spacing--1-4)"
					}
				}
			}
		},
        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--base)"
        },
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif-1)",
					"lineHeight": "var(--wp--custom--infinitum--typography--heading-line-height)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--style--block-gap)",
						"right": "0",
						"bottom": "var(--wp--preset--spacing--1-2)",
						"left": "0"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--500)",
					"fontWeight": "900"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--400)",
					"fontWeight": "700"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--300)",
					"fontWeight": "500"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--200)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--200)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--200)"
				}
			},
			"link": {
				":any-link": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline"
					},
					"outline": {
						"style": "dashed",
						"offset": "var(--wp--preset--spacing--1-4)",
						"color": "var(--wp--preset--color--accent)",
						"width": "var(--wp--preset--spacing--1-8)"
					}
				}
			}
		},
        "spacing": {
            "blockGap": "var(--wp--custom--infinitum--typography--line-height-px)",
            "margin": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            },
			"padding": {
				"top": "0",
				"right": "var(--wp--style--block-gap)",
				"bottom": "0",
				"left": "var(--wp--style--block-gap)"
			}
        },
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif-1)",
			"fontSize": "var(--wp--preset--font-size--100)",
			"lineHeight": "var(--wp--custom--infinitum--typography--line-height)"
		}
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}